<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <classPathEntry
            location="E:\MavenRepositories\mysql\mysql-connector-java\5.1.31\mysql-connector-java-5.1.31.jar" />
    <context id="context1"
             targetRuntime="com.udfex.framework.mybatis.generator.context.IntrospectedTableMyBatis3ImplExt">

        <plugin type="com.udfex.framework.mybatis.generator.plugins.ModelAnnotationPlugin" />
        <plugin type="com.udfex.framework.mybatis.generator.plugins.ModelExamplePlugin" />
        <plugin type="com.udfex.framework.mybatis.generator.plugins.MapperGeneratorPlugin" />
        <plugin type="com.udfex.framework.mybatis.generator.plugins.MapperSqlMapGeneratorPlugin" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>
        <plugin type="com.udfex.framework.mybatis.generator.plugins.PrimaryKeyGeneratorPlugin" />
        <!--<plugin type="org.mybatis.generator.plugins.CachePlugin"/>-->

        <commentGenerator>
            <property name="suppressAllComments" value="true" />
            <property name="suppressDate" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://192.168.8.200:3306/demo?useUnicode=true&amp;characterEncoding=UTF-8"
                        userId="dev" password="dev" />

        <javaTypeResolver type="com.udfex.framework.mybatis.generator.plugins.types.JavaTypeResolverDefaultImplExt"></javaTypeResolver>

        <javaModelGenerator targetPackage="com.udfex.test.mybatis.model"
                            targetProject=".\src\test\java" >
            <property name="rootClass" value="com.udfex.framework.mybatis.model.BaseMybatisModel" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.udfex.test.mybatis.mapper"
                         targetProject=".\src\test\resources" />
        <javaClientGenerator targetPackage="com.udfex.test.mybatis.mapper"
                            targetProject=".\src\test\java" type="XMLMAPPER" >
        </javaClientGenerator>

        <table schema="demo" tableName="t_department" />
        <table schema="demo" tableName="t_employee" />

    </context>
</generatorConfiguration>